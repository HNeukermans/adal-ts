{
  "name": "@hbd/adal-ts",
  "version": "1.0.1",
  "description": "adal.js typescript rewrite",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf -f -rmdir dist",
    "del-scenario": "rimraf -f -rmdir dist/scenario",
    "dev": "tsc --watch",
    "test": "karma start",
    "build": "npm run clean && tsc && npm run del-scenario",
    "webpack": "webpack",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "publish": "ci-publish",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "update": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HNeukermans/adal-ts.git"
  },
  "keywords": [
    "adal",
    "typescript"
  ],
  "author": "Hannes Neukermans, Duy Hoang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HNeukermans/adal-ts/issues"
  },
  "homepage": "https://github.com/HNeukermans/adal-ts#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/js-base64": "^2.3.1",
    "@types/lodash": "^4.14.149",
    "ci-publish": "^1.3.1",
    "core-js": "^3.6.1",
    "coveralls": "^3.0.9",
    "es6-shim": "^0.35.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jasmine-node": "^3.0.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lodash": "^4.17.15",
    "phantomjs-prebuilt": "^2.1.16",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "ts-loader": "^6.2.1",
    "tslint": "5.20.1",
    "typescript": "^3.7.4",
    "webpack": "4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {}
}
